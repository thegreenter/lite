    "keywords": ["greenter", "sunat", "xml", "facturacion electronica", "ubl21", "ubl20"],
    "license": "MIT",
    "authors": [
        {
            "name": "Giancarlos Salas",
            "email": "giansalex@gmail.com"
        }
    ],
    "homepage": "https://github.com/thegreenter/lite",
    "type": "library",
    "require": {
		"php": "^7.2",
        "ext-dom": "*",
        "greenter/xmldsig": "^5.0",
        "greenter/core": "^2.2",
        "greenter/ws": "^2.1",
